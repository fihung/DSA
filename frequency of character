#include <bits/stdc++.h>
using namespace std;
int main()
{
    int arr[26] = {0};
    char s[100];
    int n = 1;
    cin.getline(s, 99);
    for (int i = 0; s[i] != '\0'; i++) 
        ++arr[s[i]-97];
    for (int i = 0; s[i] != '\0'; i++) {
        if (arr[s[i]-97] != 0) {
            cout<<(char)s[i]<<" "<<arr[s[i]-97]<<endl;
            arr[s[i]-97] = 0;
        }
    }
    
}
/////////////////
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int x[26] = {0};
    char s1[100], s2[100];
    cin.getline(s1, 99);
    cin.getline(s2, 99);
    for (int i = 0; s2[i] != '\0'; i++)
        ++x[s2[i]-97];
    for (int i = 0; s1[i] != '\0'; i++) {
        if( x[s1[i]-97] == 0)
           cout<<s1[i];
        x[s1[i]-97]++;
    }
    
}
