#include <bits/stdc++.h>
using namespace std;
//// map
int main()
{
    int cnt = 0 ;
    int t; cin>>t;
    while(t--)
    {
        map<int, int> ma;
        int n, m; cin>>n>>; int arr1[n], arr2[m];
        for(int &x:arr1) cin>>x;
        for(int &x:arr2){
            cin>>x;
            ++ma[x];
        }
        for(int x:ma)
        if( ma.find(x) == ma.end() ) 
        ++cnt;
    }
}
