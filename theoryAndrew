#include <bits/stdc++.h>

using namespace std;
struct Node{
    int data;
    Node* next;
};
typedef struct Node* node;
Node createNode()
{
    node head = new Node();
    head -> data = x;
    head -> next = NULL; //nullptr
    return Node;
}
///kiem tra rong
bool check(node a)
{
    return a == nullptr; // nullptr = true, !nullptr = false;
}
////check kiemtra;
int sizeNode(node a)
{
    int cnt;
    while (a!= nullptr) {
        ++cnt;
        a = a->next;
    }
}
///them mot phan tu vao dau danh sach lien ket (foreinsert)
void insertNode(node &a, int x) //fact: (node*) a; // nhu kieu int a; // day la node a; == mang a;
{
    node temp = new Node();
    if ( a == nullptr ) {
        a = temp;
        return;
    }
    temp -> next = a;
    a = temp;
    
}
